   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "지름은 20\n",
      "둘레는 62.830000000000005\n",
      "넓이는 314.15000000000003\n"
     ]
    }
   ],
   "source": [
    "r = 10\n",
    "pi = 3.1415\n",
    "\n",
    "print(\"지름은\", r * 2)\n",
    "print(\"둘레는\", pi * r * 2)\n",
    "print(\"넓이는\", pi * (r **2))"
   ]
